{
  "name": "printful-sdk-js-v2",
  "version": "2.0.0-beta",
  "description": "Printful SDK for Node.js / TypeScript. A wrapper for the Printful REST API (v2)",
  "author": "Spencer Lepine <spencer.sayhello@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/spencerlepine/printful-sdk-js-v2#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencerlepine/printful-sdk-js-v2.git"
  },
  "bugs": {
    "url": "https://github.com/spencerlepine/printful-sdk-js-v2/issues"
  },
  "keywords": [
    "printful",
    "sdk",
    "javascript",
    "typescript",
    "nodejs",
    "api",
    "print-on-demand",
    "printful-sdk",
    "printful-integration",
    "printful-api",
    "node-sdk"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.mts",
  "files": [
    "dist/index.cjs.js",
    "dist/index.esm.js",
    "dist/index.d.mts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.mts"
    }
  },
  "scripts": {
    "test": "echo 'No unit tests found.' && exit 0",
    "bundle": "rollup -c",
    "generate-sdk": "openapi-generator-cli generate -c ./config/sdk-config.json",
    "generate-docs": "typedoc src/api.ts",
    "release": "npm run generate-sdk && npm run bundle && npm run generate-docs",
    "start": "npm run generate-docs && mkdir ./docs/static && docusaurus start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run release"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@docusaurus/core": "^3.4.0",
    "@docusaurus/plugin-content-docs": "^3.4.0",
    "@docusaurus/preset-classic": "^3.4.0",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^22.1.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.8.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "request": "^2.88.2",
    "rollup": "^2.79.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.3",
    "typedoc": "^0.26.5",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">= 18"
  }
}
